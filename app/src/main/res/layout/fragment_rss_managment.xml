    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".presentation.RssManagementFragment">

    <include layout="@layout/toolbar_manage_rss" android:id="@+id/toolbar_management"/>

    <TextView
